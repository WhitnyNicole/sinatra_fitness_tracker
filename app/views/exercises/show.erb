<h2>Here is your list of completed exercises!</h2>
<br><br>
  <li><strong><%= @exercise.day %></strong></li>
  <li>Category: <%= @exercise.workout.category %></li>
  <li>Duration: <%= @exercise.workout.duration %> minutes</li>
  <li>Move: <%= @exercise.move %></li>
  <li>Completed sets: <%= @exercise.sets %></li>
  <li>Completed reps: <%= @exercise.reps %></li>
  <li>Weight used: <%= @exercise.weight %> pounds</li>
  <li>Intensity: <%= @exercise.intensity %></li>
  <br><br>
<% if current_user == @exercise.workout.user %>
  <a href="/exercises/<%= @exercise.id %>/edit">Edit Exercise</a>
<br><br>
<form class="" action="/exercises/<%= @exercise.id %>" method="post">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete This Exercise">
</form>
<% end %>
